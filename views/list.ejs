    </div>

    <div class="box">
        <% for(var i=0;  i<newListItems.length; i++){ %>
        <div class="item">
            <input type="checkbox">
             <p><%= newListItems[i] %></p>
        </div>
 <% } %>

     <form action="/" method="post">
